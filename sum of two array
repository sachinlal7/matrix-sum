#include <stdio.h>
#include <conio.h>

void main()
{
    int arr1[10][10],arr2[10][10],res[10][10],i,j,m,n;
    printf("enter the size of matrix\n");
    scanf("%d %d",&m,&n);
    printf("enter the elements of first array \n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        scanf("%d",&arr1[i][j]);
    }
    printf("enter the elements of second array \n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        scanf("%d" ,&arr2[i][j]);
    }
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
      res[i][j]=arr1[i][j]+arr2[i][j];
}
    }
      printf("the resultant array is \n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
    printf("%d\t",res[i][j]);
     }
     printf("\n");
    }
}
